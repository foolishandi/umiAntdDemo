{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.2",
    "@ant-design/pro-descriptions": "^1.6.0",
    "@ant-design/pro-form": "^1.16.0",
    "@ant-design/pro-layout": "^6.5.0",
    "@ant-design/pro-table": "^2.30.0",
    "@umijs/plugin-sass": "^1.1.1",
    "antd": "^4.0.0",
    "axios": "^0.27.2",
    "classnames": "^2.2.6",
    "mockjs": "^1.1.0",
    "moment": "^2.22.2",
    "nprogress": "^0.2.0",
    "numeral": "^2.0.6",
    "nzh": "^1.0.3",
    "react": "17.x",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "17.x",
    "react-router": "^4.3.1",
    "sass": "^1.54.3",
    "sass-loader": "^13.0.2",
    "umi": "^3.5.32"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/preset-ui": "^2.2.9",
    "@umijs/test": "^3.5.32",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
